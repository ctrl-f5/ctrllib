        "zendframework/zend-eventmanager": "2.0.*",
        "zendframework/zend-filter": "2.0.*",
        "zendframework/zend-i18n": "2.0.*",
        "zendframework/zend-view": "2.0.*",
        "zendframework/zend-config": "2.0.*",
        "zendframework/zend-form": "2.0.*",
        "zendframework/zend-inputfilter": "2.0.*",
        "zendframework/zend-modulemanager": "2.0.*",
        "zendframework/zend-session": "2.0.*",
        "doctrine/doctrine-orm-module": "dev-master",
        "doctrine/doctrine-module": "dev-master"
